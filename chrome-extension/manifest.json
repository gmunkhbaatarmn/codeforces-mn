{
  "name": "Codeforces Mongolia",
  "short_name": "Codeforces.mn",
  "version": "0.2.6",
  "manifest_version": 2,
  "description": "Codeforces.com Mongolian translation",
  "homepage_url": "http://github.com/gmunkhbaatarmn/codeforces-mn",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "http://codeforces.mn/*",
    "http://codeforces.com/*"
  ],
  "content_scripts": [{
    "all_frames": true,
    "run_at": "document_start",
    "matches": ["http://codeforces.com/*"],
    "js": ["jquery.js", "main.js"]
  }]
}
